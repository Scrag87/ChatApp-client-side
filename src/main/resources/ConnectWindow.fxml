<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
  prefHeight="299.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="ConnectionController">
  <children>
    <Button fx:id="butCloseApp" layoutX="29.0" layoutY="243.0" mnemonicParsing="false"
      onAction="#actionClose" prefHeight="44.0" prefWidth="61.0" text="&lt;Back"/>
    <Button fx:id="butConnect" disable="true" layoutX="228.0" layoutY="245.0"
      mnemonicParsing="false" onAction="#actionConnect" prefHeight="44.0" prefWidth="136.0"
      text="Connect"/>
    <Label layoutX="25.0" layoutY="55.0" text="Server IP " AnchorPane.leftAnchor="25.0"
      AnchorPane.topAnchor="55.0"/>
    <TextField fx:id="ipAddress" layoutX="25.0" layoutY="75.0" text="127.0.0.1"
      AnchorPane.leftAnchor="25.0" AnchorPane.topAnchor="75.0"/>
    <TextField fx:id="port" layoutX="25.0" layoutY="150.0" prefHeight="26.0" prefWidth="64.0"
      text="8087" AnchorPane.leftAnchor="25.0" AnchorPane.topAnchor="150.0"/>
    <Label layoutX="25.0" layoutY="130.0" text="Port" AnchorPane.leftAnchor="25.0"
      AnchorPane.topAnchor="130.0"/>
    <Label fx:id="connectionCheckLabel" layoutX="25.0" layoutY="210.0" prefHeight="25.0"
      prefWidth="268.0" AnchorPane.leftAnchor="25.0" AnchorPane.topAnchor="210.0"/>
    <Button fx:id="butCheckConnection" layoutX="232.0" layoutY="75.0" mnemonicParsing="false"
      onAction="#actionCheckConnection" prefHeight="44.0" prefWidth="136.0"
      text="Check Connection"/>
  </children>
</AnchorPane>
